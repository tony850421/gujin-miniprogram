<!--index.wxml-->
<view class='code'>
  <image bindtap='scanQRCode' src='../../images/qrcode.png' class='qrImage'></image>
</view>
<view class="section">
  <view class="ImageHome">
    <image class='HomePicture' src="http://lc-qNFjWh6z.cn-n1.lcfile.com/c8ea64d214285c5a4aee.jpg"></image>    
  </view>  
</view>

<view class="section">
  <view class="buttonsTop">
    <view class='comerceBox'>
      <image bindtap='goToComerce' mode='aspectFit' class='IconButtonComerce' src='../../images/comerce.png'></image>
      <text>Comerce</text>
    </view>
    <view class='comerceBox'>
      <image bindtap='goToHealth' mode='aspectFit' class='IconButtonHealth' src='../../images/health.png'></image>
      <text>Health</text>
    </view>
    <view class='comerceBox'>
      <image mode='widthFix' class='IconButtonTourism' src='../../images/travel.png'></image>
      <text>Tourism</text>
    </view>
  </view>
</view>
<view class="section">
  <view class='boxImageProduct'>
    <view class='imageProductBox'>
      <image mode='widthFix' class="imageProduct" src='http://lc-qNFjWh6z.cn-n1.lcfile.com/11ea5bbca5cb7d7c97be.jpg'></image>
      <text>Habanos</text>
    </view>
    <view  class='imageProductBox'>
      <image mode='widthFix' class="imageProduct" src='http://lc-qNFjWh6z.cn-n1.lcfile.com/80d89c9ee8bda9436ea0.jpg'></image>
      <text>Habana Club</text>
    </view>
  </view>
</view>
<view class='section'>
  <view class="imageMargin">
    <video class='videoHome' src="http://lc-qNFjWh6z.cn-n1.lcfile.com/a83666d30482b79683b4.mp4" controls ></video>
  </view>
</view>
<view class="section">
  <view class='boxImageProduct'>
    <view class='imageProductBox'>
      <image mode='widthFix' class="imageProduct" src='http://lc-qNFjWh6z.cn-n1.lcfile.com/3aca4f015b846c455e64.jpg'></image>
      <text>Havana</text>
    </view>
    <view  class='imageProductBox'>
      <image mode='widthFix' class="imageProduct" src='http://lc-qNFjWh6z.cn-n1.lcfile.com/faeb3c8035c358499fcf.jpg'></image>
      <text>Women</text>
    </view>
  </view>
</view>