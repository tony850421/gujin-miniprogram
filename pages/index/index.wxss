  margin-top: 10px;
}

.TextMarginBottom{
  margin-top: 10px;
  margin-bottom:20px;
}

.videoHome{
  width: 100%;
}

.comerceBox{
  display: flex;
  flex-direction: column;
  text-align: center;
  align-items: center;
  width: 33.3%;
}

.imageProductBox{
  display: flex;
  flex-direction: column;
  align-items: center;
  text-align: center;
  width: 100%;
  padding: 15px;
}

.boxImageProduct{
  display: flex;
  flex-direction: row;
  margin-top: 20px;
}

.imageProduct{
  width: inherit !important;
  margin-bottom: 10px;
}

