}

.buttonsTop{
  margin-top: 10px;
  flex-direction: row;
  display: flex;
  align-items: center;
  text-align: center;
}

.buttonList{
  margin-left: 50px;
  border-radius: 50px;
  border: 0px;
}

.imageMargin{
  margin-top: 30px;
}

